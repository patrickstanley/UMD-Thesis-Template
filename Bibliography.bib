issn = {03787753},
journal = {J. Power Sources},
keywords = {Chemo-mechanical coupling,Creep,Ni-YSZ,Reduction,SOEC,SOFC},
pages = {78--89},
title = {{Accelerated creep in solid oxide fuel cell anode supports during reduction}},
volume = {323},
year = {2016}
}
