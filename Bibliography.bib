doi = {10.1016/j.jpowsour.2009.10.099},
file = {:Users/pstanley/Documents/Mendeley/Laurencin et al/Journal of Power Sources/Laurencin et al. - 2010 - Impact of 'redox' cycles on performances of solid oxide fuel cells Case of the electrolyte supported cells.pdf:pdf},
isbn = {0378-7753},
